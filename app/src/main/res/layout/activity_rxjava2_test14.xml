<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".rxjava2.blog2.Rxjava2Test14Activity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <Button
                android:id="@+id/btn_test1"
                style="@style/btn_style_lower"
                android:text="filter()"
                />

            <Button
                android:id="@+id/btn_test2"
                style="@style/btn_style_lower"
                android:text="onType()"
                />

            <Button
                android:id="@+id/btn_test3"
                style="@style/btn_style_lower"
                android:text="skip()"
                />

            <Button
                android:id="@+id/btn_test4"
                style="@style/btn_style_lower"
                android:text="distinct()"
                />

            <Button
                android:id="@+id/btn_test5"
                style="@style/btn_style_lower"
                android:text="distinctUntilChanged()"
                />

            <Button
                android:id="@+id/btn_test6"
                style="@style/btn_style_lower"
                android:text="take()"
                />

            <Button
                android:id="@+id/btn_test7"
                style="@style/btn_style_lower"
                android:text="debounce()"
                />

            <Button
                android:id="@+id/btn_test8"
                style="@style/btn_style_lower"
                android:text="firstElement()"
                />

            <Button
                android:id="@+id/btn_test9"
                style="@style/btn_style_lower"
                android:text="lastElement()"
                />

            <Button
                android:id="@+id/btn_test10"
                style="@style/btn_style_lower"
                android:text="elementAt()"
                />

            <Button
                android:id="@+id/btn_test11"
                style="@style/btn_style_lower"
                android:text="elementAtError()"
                />

        </LinearLayout>
    </ScrollView>

</android.support.constraint.ConstraintLayout>